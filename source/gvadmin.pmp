			session_id($sessionid);
			session_start();
		}
		if (@$_COOKIE[PROJECT_NAME]['AutoLogin'] == "") // Not autologin
		setcookie(PROJECT_NAME . '[Username]', ""); // Clear user name cookie
		setcookie(PROJECT_NAME . '[Password]', ""); // Clear password cookie
		setcookie(PROJECT_NAME . '[LastUrl]', ""); // Clear last URL
		// Clean upload temp folder
		CleanUploadTempPaths(session_id());
		// Unset all of the Session variables
		$_SESSION = array();
		// Delete the Session cookie and kill the Session
		if (isset($_COOKIE[session_name()]))
		setcookie(session_name(), '', time()-42000, '/');
